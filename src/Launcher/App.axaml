<Application xmlns="https://github.com/avaloniaui"
             xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
             x:Class="Launcher.App"
             xmlns:local="using:Launcher"
             RequestedThemeVariant="Dark">

  <Application.Resources>
    <ResourceDictionary>
      <ResourceDictionary.MergedDictionaries>
        <ResourceInclude Source="/Resources/Icons.axaml"/>
        <ResourceInclude Source="/Resources/Colors.axaml"/>
      </ResourceDictionary.MergedDictionaries>

      <ResourceInclude x:Key="en_US" Source="/Resources/Locales/en_US.axaml"/>
    </ResourceDictionary>
  </Application.Resources>

  <Application.Styles>
    <FluentTheme />
    <StyleInclude Source="/Resources/Styles.axaml" />
	<StyleInclude Source="avares://AsyncImageLoader.Avalonia/AdvancedImage.axaml" />
  </Application.Styles>
	
	<Application.Resources>
		<FontFamily x:Key="FontDefault">/Assets/Fonts/Maiandra-GD-Regular.otf#Maiandra GD</FontFamily>
	</Application.Resources>
	
</Application>